class Solution {
public:
    bool hasSameDigits(string s) {

        int n=s.size();
        while(n>2){
            string str="";
            for(int i=0;i<n-1;i++){

                int sum=(s[i]-'0'+s[i+1]-'0')%10;
                str.push_back(sum+'0');
                
            }
            s=str;
            n=s.size();
        }
        return s[0]==s[1];
        
    }
};
