class Solution {
public:
    int alternatingSum(vector<int>& nums) {

        int evenIndexSum=0;
        int oddIndexSum=0;
        for(int i=0;i<nums.size();i++){
            if(i%2==0) evenIndexSum=evenIndexSum+nums[i];
            else oddIndexSum=oddIndexSum+nums[i];
        }
        return evenIndexSum-oddIndexSum;
        
    }
};
