class Solution {
public:
    vector<int> arrayRankTransform(vector<int>& arr) {
        vector<int> v(arr);
        sort(v.begin(),v.end());
        int rank=1;
        unordered_map<int,int> mp;
        for(auto num:v){
            if(mp.find(num)==mp.end()){
                mp[num]=rank;
                rank++;
            }
        }

        vector<int> ans;
        for(auto num:arr){
            ans.push_back(mp[num]);
        }
        return ans;
        
        
    }
};
