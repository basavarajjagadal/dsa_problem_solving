class Solution {
public:
    int countStudents(vector<int>& students, vector<int>& sandwiches) {

        int arr[2];
        for(auto student:students){
            arr[student]++;
        }
        int n=sandwiches.size();
        for(int i=0;i<n;i++){
            int sand=sandwiches[i];
            if(arr[sand]==0) return n-i;
            arr[sand]--;
        }
        return 0;
        
    }
};
