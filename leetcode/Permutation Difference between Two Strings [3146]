class Solution {
public:
    int findPermutationDifference(string s, string t) {
        unordered_map<int,int> mpp;
        for(int i=0;i<t.size();i++){
            mpp[t[i]]=i;
        }
        int sum=0;
        for(int i=0;i<s.size();i++){
            int diff=abs(i-mpp[s[i]]);
            sum=sum+diff;
        }
        return sum;
        
    }
};
