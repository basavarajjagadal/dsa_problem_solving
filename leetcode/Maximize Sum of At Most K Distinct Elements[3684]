class Solution {
public:
    vector<int> maxKDistinct(vector<int>& nums, int k) {


        vector<int> ans;
        sort(nums.begin(),nums.end());
        int n=nums.size();
        unordered_set<int> st;
        for(int i=n-1;i>=0;i--){
            if(st.find(nums[i])==st.end()){
                ans.push_back(nums[i]);
                k--;
            }
            if(k==0){
                break;
            }
            st.insert(nums[i]);
        }
        return ans;     
    }
};
