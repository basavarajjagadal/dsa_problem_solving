class Solution {
public:
    int areaOfMaxDiagonal(vector<vector<int>>& dimensions) {

        int maxDiagSq = 0;   
        int maxArea = 0;

        for(auto &x : dimensions) {
            int length = x[0];
            int width = x[1];
            int diagSq = length*length + width*width;
            int area = length * width;

            if (diagSq > maxDiagSq || (diagSq == maxDiagSq && area > maxArea)) {
                maxDiagSq = diagSq;
                maxArea = area;
            }
        }
        return maxArea;
        
    }
};
