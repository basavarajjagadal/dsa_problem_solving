class Solution {
public:
    vector<int> getSneakyNumbers(vector<int>& nums) {
        vector<int> hash(101,0);
        vector<int> ans;
        for(auto num:nums){
            hash[num]++;
            if(hash[num]>1){
                ans.push_back(num);
            }
        }
        return ans;   
    }
};
