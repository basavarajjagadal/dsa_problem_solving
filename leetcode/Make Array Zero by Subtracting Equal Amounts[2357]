class Solution {
public:
    int minimumOperations(vector<int>& nums) {

        int operations=0;
        sort(nums.begin(),nums.end());
        for(int i=0;i<nums.size();i++){
            if(nums[i]==0) continue;
            int lowest=nums[i];
            for(int j=i+1;j<nums.size();j++){
                if(nums[j]==0) continue;
                nums[j]=nums[j]-lowest;
            }
            operations++;
        }
        return operations;
    }
};
