class Solution {
public:
    long long sumAndMultiply(int n) {
        
        long long ans=0;
        long long place=1;
        long long sum=0;
        while(n!=0){
            int digit=n%10;
            if(digit!=0){
                sum=sum+digit;
                ans=digit*place+ans;
                place=place*10;
            }
            n=n/10;
        }
        return ans*sum;

        
    }
};
