class Solution {
public:
    vector<string> removeAnagrams(vector<string>& words) {
        vector<string> ans;
        for(auto word:words){
            if(!ans.empty()){
                string str1=ans.back();
                string str2=word;
                sort(str1.begin(),str1.end());
                sort(str2.begin(),str2.end());
                if(str1==str2) continue;

            }    
            ans.push_back(word);
        }
        return ans;
    }
};
