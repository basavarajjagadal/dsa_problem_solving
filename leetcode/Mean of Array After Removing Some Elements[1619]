class Solution {
public:
    double trimMean(vector<int>& arr) {

        sort(arr.begin(),arr.end());
        int n=arr.size();
        int skip=(n*5)/100;
        int end=n-skip-1;
        int start=skip;

        double sum=0;
        for(int i=start;i<=end;i++){
            sum=sum+arr[i];
        }
        int remainingelements=n-2*skip;
        return sum/remainingelements;

        
    }
};
