class Solution {
public:
    int smallestAbsent(vector<int>& nums) {

        float sum=0;
        unordered_set<int> st;
        for(auto num:nums){
            sum=sum+num;
            st.insert(num);
        }
        float avg=sum/nums.size();

        for(int i=1;i<=10000;i++){
            if(i>avg && st.find(i)==st.end()){
                return i;
            }
        }
        return -1;
    }
};
