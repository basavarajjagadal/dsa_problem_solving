class Solution {
public:
    bool kLengthApart(vector<int>& nums, int k) {
        int zerocount=0;
        bool  flag=false;
        for(int i=0;i<nums.size();i++){
            if(nums[i]==0) zerocount++;
            else{
                if(zerocount<k && flag==true){
                    return false;
                }
                flag=true;
                zerocount=0;
            }
        }
        return true;
        
    }
};
