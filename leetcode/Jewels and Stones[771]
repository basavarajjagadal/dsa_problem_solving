class Solution {
public:
    int numJewelsInStones(string jewels, string stones) {

        unordered_map<char,int> mp;
        for(auto &ch:jewels){
            mp[ch]++;
        }
        int count=0;
        for(auto &ch:stones){
            if(mp.find(ch)!=mp.end()){
                count++;
            }
        }
        return count;
 
    }
};
