class Solution {
public:
    vector<int> sortArrayByParity(vector<int>& nums) {
        vector<int> ans(nums.size());
        int i=0;
        int j=nums.size()-1;
        for(auto num:nums){
            if(num%2==0) ans[i++]=num;
            else ans[j--]=num;
        }
        return ans;
    }
};
