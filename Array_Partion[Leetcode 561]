class Solution {
public:
    int arrayPairSum(vector<int>& nums) {
        int sum=0;                      //to hold answer
        sort(nums.begin(),nums.end());  //to maintain in ascending order
        for(int i=0;i<nums.size();i+=2){  //jumps +2
            sum=sum+nums[i];              //addingsum
        }
        return sum;                       //return ans
    }
};
